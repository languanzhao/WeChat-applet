.container{
	height:80rpx;
	display: flex;
	justify-content: space-around;
	margin: 30rpx 0;
	line-height: 80rpx;
}
.container view{
	font-size: 30rpx;
}
button[type=primary]{
	background: #2DB7F5;
	width: 80rpx;
	height: 80rpx;
	border-radius: 50%;
	margin: 0;
	padding: 0;
	font-size: 26rpx;
	line-height: 80rpx;
	color: #fff;
}
button[type=primary]:after{
	border: none;
}
button-hover[type=primary]{
	background: #fff;
}
.progress-item{
	display: flex;
	justify-content: space-around;
	align-items: center;
}
i-progress{
	width: 80%;
}
i-button{
	font-size: 60rpx;
	font-weight: bold;
	width: 80rpx;
	text-align: center;
	color: #999;
}