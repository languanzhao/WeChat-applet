.container{
	width: 90%;
	height: 200rpx;
	border-bottom: 1rpx solid #CCC;
	margin: 30rpx auto;
}
.container .date{
	color: #DDD;
	text-align: center;
	font-size: 30rpx;
	display: block;
	margin-bottom: 20rpx;
}
.container navigator{
	display: block;
	width: 100%;
	height: 120rpx;
	box-sizing:border-box;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.container navigator text{
	display: block;
	width:500rpx;
	font-size: 30rpx;
	line-height: 50rpx;
}
.container navigator image{
	width: 120rpx;
	height: 120rpx;
}
.loading{
	width: 200rpx;
	height: 200rpx;
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	text-align: center;
}
.loadingImg{
	width:54rpx;
	height:54rpx;
	display: block;
	margin:5rpx auto;
}
.loading text{
	font-size: 30rpx;
}